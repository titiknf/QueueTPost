<body>
  <% include menu.ejs %>
 
   <div class="container"  style="padding-top: 50px;">

   	   	<div class='row'>
   		<div class='col-md-12'>
		<div class="panel panel-default">
			<div class="panel-heading">
				<h3>รายงานแสดงรายวัน</h3>
			</div>
			<div class="panel-body">
				<div class="form-group row">
					<label for="txtStartDate" class="col-md-1 control-label text-rigth" style='text-align: right;margin-top: 5px'>วันที่ </label>
					<div class="form-group col-md-2">
					  <input type="text" class="form-control" id="txtStartDate" >
					</div>
					<label for="txtEndDate" class="col-md-1 control-label text-rigth" style='text-align: right;margin-top: 5px'>ถึง วันที่ </label>
					<div class="form-group col-md-2">
					  <input type="text" class="form-control" id="txtEndDate" >
					</div>
					<div class="col-md-offset-3 col-md-3" style='text-align: right; '>
						<input type="button"  id="btnSearch" style='margin-left: 10px' class="btn btn-info" value="Search">
						<input type="button"  id="btnExport" style='margin-left: 10px' class="btn btn-success" value="Export XLS">
					</div>
          		</div>
					

			 	<div class="row">
     				<div class="col-md-12">
				        <table class='table table-bordered table-strip table-condensed' id='tbResult'>
					       <thead class='bg-info'>
					          	<tr>
					          		<th colspan='4' >รายงานสรุป <span id='lbStart'></span> - <span id='lbEnd'></span></th>
					        	</tr>
					        	<tr>
					        		<th>Counter</th>
					        		<th>จำนวนคิวที่ให้บริการ</th> 
					        		<th>เวลารอ</th>
					        		<th>เวลาให้บริการ</th>
					        	</tr>
				 			</thead>
							 <tbody></tbody>
						</table>
					</div>  
			
				</div>
			</div>
		</div>
		</div>
		</div>
</body>
</html>