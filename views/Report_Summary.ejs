<head>

    <meta charset="UTF-8">
    <title><%= title %></title>
	 <link rel="Stylesheet" href="/stylesheets/bootstrap.min.css" />
\
    <link rel="Stylesheet" href="/stylesheets/template.css" />
    <link rel="Stylesheet" href="/stylesheets/menustyles.css" />
    <link rel="Stylesheet" href="/stylesheets/font-awesome/css/font-awesome.min.css" />
    <link rel="Stylesheet" type="text/css" media="all" href="/stylesheets/daterangepicker.css" />
 
    <script src="/javascripts/lib/jquery-2.1.4.min.js" type="text/javascript"></script>
    <script type="text/javascript" src="/javascripts/moment.js"></script>
    <script type="text/javascript" src="/javascripts/date.js"></script>

    <script type="text/javascript" src="/javascripts/daterangepicker.js"></script>
    <script src="/views/report_summary_views.js"></script>

    <script type="text/javascript">
       var host = '<%= host %>';
       var frm = new  clsReport_Summary_Views();
       frm.Fn_Initial();
    </script>
